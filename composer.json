{
    "name": "elazar/auryn-container-interop",
    "description": "PSR-11 Container compatibility for Auryn",
    "license": "MIT",
	"support": {
        "email": "me@matthewturland.com",
        "issues": "https://github.com/elazar/auryn-container-interop/issues",
        "source": "https://github.com/elazar/auryn-container-interop"
    },
    "authors": [
        {
            "name": "Matthew Turland",
            "homepage": "http://matthewturland.com"
        }
    ],
    "keywords": [
        "dependency injection",
        "PSR-11",
        "Auryn"
    ],
    "require": {
        "rdlowrey/auryn": "^1.4",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5"
    },
    "autoload": {
        "psr-4": {
            "Elazar\\Auryn\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elazar\\Auryn\\": "tests"
        }
    },
    "scripts": {
        "test": "php vendor/bin/phpunit"
    }
}
